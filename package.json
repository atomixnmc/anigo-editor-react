    "start": "better-npm-run start",
    "start-prod": "better-npm-run start-prod",
    "build": "better-npm-run build",
    "build-prod": "better-npm-run build-prod",
    "test": "karma start",
    "test-node": "./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register",
    "test-node-watch": "./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register --watch"
  },
  "betterScripts": {
    "start": {
      "command": "webpack-dev-server --config webpack/webpack.config.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3100,
        "APIPORT": 4000
      }
    },
    "start-prod": {
      "command": "webpack-dev-server --config webpack/webpack.config.prod.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 3100,
        "APIPORT": 4000
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/webpack.config.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3100,
        "APIPORT": 4000
      }
    },
    "build-prod": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/webpack.config.prod.js",
      "env": {
        "NODE_ENV": "production",
        "APIHOST": "fshidemo-hr-college-api.herokuapp.com",
        "APIPORT": "none"
      }
    }
  },
